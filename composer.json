{
    "name": "carlosafonso/google-maps-php",
    "type": "library",
    "description": "Unofficial PHP SDK for the Google Maps API",
    "keywords": [
        "google",
        "maps"
    ],
    "homepage": "https://github.com/carlosafonso/google-maps-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Afonso",
            "email": "carlos.afonso.perez@e-valua.es"
        }
    ],
    "require": {
        "php": "~5.5|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "Afonso\\GoogleMaps\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Afonso\\GoogleMaps\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
